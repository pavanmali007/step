<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <servlet>
        <servlet-name>FirstServlet</servlet-name>
        <servlet-class>com.pavan.FirstServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SecondServlet</servlet-name>
        <servlet-class>com.pavan.SecondServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DashboardCheck</servlet-name>
        <servlet-class>com.pavan.DashboardCheck</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>NavbarCheck</servlet-name>
        <servlet-class>com.pavan.NavbarCheck</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>FirstServlet</servlet-name>
        <url-pattern>/add</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SecondServlet</servlet-name>
        <url-pattern>/sq</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DashboardCheck</servlet-name>
        <url-pattern>/DashCheck.fin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>NavbarCheck</servlet-name>
        <url-pattern>/NavCheck.fin</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
</web-app>
